{
  "manifest_version": 2,

  "name": "Feline Extension",
  "description": "Make every image display a marvelous and majestic cat!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Replacer"
  },
  "background": {
    "persistent": true
  },
  "permissions":[
    "tabs"
  ],
  "content_scripts": [
  {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["myscript.js"]
  }
]
}
